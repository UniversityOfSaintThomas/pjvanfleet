  3.732355530720536*^9}},ExpressionUUID->"eb6fcfe8-b56f-4fc4-bdae-\
fe6b2c229f72"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["W", "~"], "N"], TraditionalForm]],ExpressionUUID->
  "a8dc0f65-f6ef-4f2d-b0eb-24c097ea2eb8"],
 StyleBox["v",
  FontWeight->"Bold"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       SubscriptBox[
        OverscriptBox["H", "~"], 
        RowBox[{"N", "/", "2"}]]},
      {
       SubscriptBox[
        OverscriptBox["G", "~"], 
        RowBox[{"N", "/", "2"}]]}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "6b600c93-41c3-4b9a-b086-1a7d74016622"],
 "  ",
 StyleBox["v",
  FontWeight->"Bold"],
 " =  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       RowBox[{
        SubscriptBox[
         OverscriptBox["H", "~"], 
         RowBox[{"N", "/", "2"}]], 
        StyleBox["v",
         FontWeight->"Bold"]}]},
      {
       RowBox[{
        SubscriptBox[
         OverscriptBox["G", "~"], 
         RowBox[{"N", "/", "2"}]], 
        StyleBox["v",
         FontWeight->"Bold"]}]}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "2c4df5e3-ed57-4fd9-a038-9323a96fd98a"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       StyleBox["a",
        FontWeight->"Bold",
        FontSlant->"Plain"]},
      {
       StyleBox["d",
        FontWeight->"Bold",
        FontSlant->"Plain"]}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "2f27b52a-a626-4564-9752-84a868ecbfe4"],
 "."
}], "Text",
 CellChangeTimes->{{3.730221367009367*^9, 3.730221439710001*^9}, {
