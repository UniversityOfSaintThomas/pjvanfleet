Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "h", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"hh", "=", 
   RowBox[{"Array", "[", 
    RowBox[{"h", ",", "6", ",", "0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"eqs", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"Total", "[", 
       RowBox[{"hh", "^", "2"}], "]"}], "\[Equal]", "1"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"h", "[", "0", "]"}], "*", 
        RowBox[{"h", "[", "2", "]"}]}], "+", 
       RowBox[{
        RowBox[{"h", "[", "1", "]"}], "*", 
        RowBox[{"h", "[", "3", "]"}]}], "+", 
       RowBox[{
        RowBox[{"h", "[", "2", "]"}], "*", 
        RowBox[{"h", "[", "4", "]"}]}], "+", 
       RowBox[{
        RowBox[{"h", "[", "3", "]"}], "*", 
        RowBox[{"h", "[", "5", "]"}]}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"h", "[", "0", "]"}], "*", 
        RowBox[{"h", "[", "4", "]"}]}], "+", 
       RowBox[{
        RowBox[{"h", "[", "1", "]"}], "*", 
        RowBox[{"h", "[", "5", "]"}]}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"Rest", "[", "hh", "]"}], ".", 
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "2"}], ",", "3", ",", 
         RowBox[{"-", "4"}], ",", "5"}], "}"}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"Rest", "[", "hh", "]"}], ".", 
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "4"}], ",", "9", ",", 
         RowBox[{"-", "16"}], ",", "25"}], "}"}]}], "\[Equal]", "0"}], ",", 
     RowBox[{
      RowBox[{"Total", "[", "hh", "]"}], "\[Equal]", 
      RowBox[{"Sqrt", "[", "2", "]"}]}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"TableForm", "[", "eqs", "]"}]}], "Input",
 CellChangeTimes->{{3.7301988165362253`*^9, 3.730198821099764*^9}, {
