Cell[TextData[{
 "We first consider the computation ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["H", "N", "T"], TraditionalForm]],ExpressionUUID->
  "a279e979-218a-4807-8a7b-e6a3e0b676d2"],
 StyleBox["a",
  FontWeight->"Bold"],
 ".  The cell below gives some insight for ",
 StyleBox["a",
  FontWeight->"Bold"],
 " of length 8."
}], "Text",
 CellChangeTimes->{{3.7335861906844635`*^9, 
  3.7335862243953094`*^9}},ExpressionUUID->"48357dbd-d0b5-445c-8ac6-\
d92189ef2012"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"n", "-", "8"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Clear", "[", "a", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"aa", "=", 
   RowBox[{"Array", "[", 
    RowBox[{"a", ",", "n"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"r", "=", 
   RowBox[{"PadRight", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{"1", "/", "2"}]}], "}"}], ",", "n"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"H", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"RotateRight", "[", 
       RowBox[{"r", ",", "#"}], "]"}], "&"}], ",", 
     RowBox[{"Range", "[", 
      RowBox[{"0", ",", 
       RowBox[{"n", "-", "1"}]}], "]"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"TableForm", "[", 
  RowBox[{
   RowBox[{"Transpose", "[", "H", "]"}], ".", "aa"}], "]"}]}], "Input",
 CellChangeTimes->{{3.7335862305219927`*^9, 3.7335862952592278`*^9}, {
  3.733589144966337*^9, 
  3.7335891450757284`*^9}},ExpressionUUID->"6aadbcf8-66c3-4774-9884-\
48e7538b672b"],

Cell["We can rewrite this product as", "Text",
 CellChangeTimes->{{3.7335863129461775`*^9, 
  3.7335863180893545`*^9}},ExpressionUUID->"6592ccf4-7403-4cc2-ad1f-\
84f1d66b3bfc"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       SubscriptBox["a", "8"], 
       SubscriptBox["a", "1"]},
      {
       SubscriptBox["a", "1"], 
       SubscriptBox["a", "2"]},
      {
       SubscriptBox["a", "2"], 
       SubscriptBox["a", "3"]},
      {
       SubscriptBox["a", "3"], 
       SubscriptBox["a", "4"]},
      {
       SubscriptBox["a", "4"], 
       SubscriptBox["a", "5"]},
      {
       SubscriptBox["a", "5"], 
       SubscriptBox["a", "6"]},
      {
       SubscriptBox["a", "6"], 
       SubscriptBox["a", "7"]},
      {
       SubscriptBox["a", "7"], 
       SubscriptBox["a", "8"]}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "ddd0bc27-54e8-4541-8b6e-1e0bf2f6af20"],
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       FractionBox["1", "2"]},
      {
       FractionBox["1", "2"]}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "72d572d5-1350-4a1b-a699-e3e083f7c249"],
 " = A",
 StyleBox["h",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.7335863249180098`*^9, 3.7335864590007687`*^9}, {
  3.733587030944956*^9, 
  3.7335870325683928`*^9}},ExpressionUUID->"4be31218-5646-47f3-844a-\
d615acd84832"],

Cell[TextData[{
 "Thus we need to partition ",
 StyleBox["a",
  FontWeight->"Bold"],
 " as an Nx2 matrix and multiply it by lowpass filter ",
 StyleBox["h",
  FontWeight->"Bold"],
 ".  In a similar way, you should verify that ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["G", "N", "T"], TraditionalForm]],ExpressionUUID->
  "17e6e9fc-8a92-4183-98f1-6f6f080b339f"],
 StyleBox["d",
  FontWeight->"Bold"],
 " can be written as"
}], "Text",
