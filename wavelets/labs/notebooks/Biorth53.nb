405ed4e33e2e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"InverseBWT531d1", "[", "y_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "h", ",", "ht", ",", "g", ",", "a", ",", "d", ",", "A1", ",", "A2", ",",
        "he", ",", "ho", ",", "x1", ",", "x2"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"h", ",", "ht"}], "}"}], "=", 
       RowBox[{"SplineFilters", "[", 
        RowBox[{"2", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "replace", " ", "this", " ", "comment", " ", "with", " ", "your", " ", 
        "code"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"Chop", "[", 
        RowBox[{"x1", "+", "x2"}], "]"}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
